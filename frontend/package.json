{
  "name": "@kiali/kiali-ui",
  "version": "2.14.0",
  "description": "React UI for [Kiali](https://github.com/kiali/kiali).",
  "keywords": [
    "istio service mesh",
    "kiali",
    "monitoring",
    "observability",
    "okd",
    "openshift"
  ],
  "homepage-comment": [
    "*Do not* change this setting if you wish run Kiali under different server root.",
    "Instead update 'web_root' in Kaili config map in OpenShift console."
  ],
  "homepage": "./",
  "bugs": {
    "url": "https://github.com/kiali/kiali/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiali/kiali-ui.git"
  },
  "license": "Apache-2.0",
  "author": "Red Hat",
  "main": "index.js",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "script-comments": [
    "When adding new scripts, please be careful to using `npm run` instead of `yarn` for the tasks.",
    "Some build environments we use do not include npm access, and installing yarn is not possible."
  ],
  "scripts": {
    "build": "if [ \"${KIALI_ENV}\" = \"production\" ]; then npm run build:prod; else npm run build:dev; fi",
    "build:dev": "sh -ac '. ./.env.upstream; npm run lint && npm run build:kiali'",
    "build:kiali": "npm run i18n && REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name GENERATE_SOURCEMAP=false EXTEND_ESLINT=true react-scripts build --profile",
    "build:prod": "sh -ac '. ./.env.downstream; npm run build:kiali'",
    "cypress": "cypress open -e TAGS=\"not @multi-cluster and not @multi-primary and not @external-kiali and not @ambient and not @waypoint and (not @waypoint-tracing or @tracing)\"",
    "cypress:ambient": "cypress open -e TAGS=\"@ambient\"",
    "cypress:external-kiali": "cypress open -e TAGS=\"@external-kiali\"",
    "cypress:waypoint": "cypress open -e TAGS=\"@waypoint or @waypoint-tracing\"",
    "cypress:combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "cypress:delete:reports": "rm cypress/results/* || true",
    "cypress:multi-cluster": "cypress open -e TAGS=\"@multi-cluster and not @multi-primary\"",
    "cypress:multi-primary": "cypress open -e TAGS=\"@multi-cluster and @multi-primary\"",
    "cypress:perf": "cypress open --config-file cypress-perf.config.ts",
    "cypress:run": "cypress run -e TAGS=\"@smoke\" && cypress run -e TAGS=\"not @crd-validation and not @multi-cluster and not @multi-primary and not @external-kiali and not @smoke and not @ambient and not @waypoint and (not @waypoint-tracing or @tracing)\" && cypress run -e TAGS=\"@crd-validation\"",
    "cypress:run:ambient": "cypress run -e TAGS=\"@ambient\" && cypress run -e TAGS=\"@waypoint\" && cypress run -e TAGS=\"@waypoint-tracing\"",
    "cypress:run:ambient123": "cypress run -e TAGS=\"@ambient\" && cypress run -e TAGS=\"@waypoint and not @skip-istio-1-23\" && cypress run -e TAGS=\"@waypoint-tracing\"",
    "cypress:run:junit": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json -e TAGS=\"not @crd-validation and not @multi-cluster and not @multi-primary and not @ambient and not @waypoint and (not @waypoint-tracing or @tracing)\" && cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json -e TAGS=\"@crd-validation\"",
    "cypress:run:external-kiali": "cypress run -e TAGS=\"@external-kiali\"",
    "cypress:run:multi-cluster": "cypress run -e TAGS=\"@multi-cluster and not @multi-primary\"",
    "cypress:run:multi-primary": "cypress run -e TAGS=\"@multi-cluster and @multi-primary\"",
    "cypress:run:perf": "cypress run --config-file cypress-perf.config.ts",
    "cypress:run:selected": "cypress run -e TAGS=\"@selected\"",
    "cypress:run:smoke": "cypress run -e TAGS=\"@smoke\"",
    "cypress:run:test-group:junit": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json -e TAGS=\"${TEST_GROUP}\"",
    "cypress:run:tracing": "cypress run -e TAGS=\"@tracing\"",
    "cypress:selected": "cypress open -e TAGS=\"@selected\"",
    "cypress:tracing": "cypress open -e TAGS=\"@tracing\"",
    "format:precommit": "../hack/hooks/pre-commit.sh",
    "i18n": "i18next 'src/**/*.{ts,tsx}' -c './i18next-parser.config.ts' --silent",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "lint:precommit": "eslint -c .eslintrc_precommit $(git diff --staged --name-only HEAD --diff-filter=AM | grep -E '\\.tsx?$' | cut -d'/' -f2-)",
    "pre-commit": "yarn run pretty-quick --staged --no-restage --bail --pattern \"**/*.{ts,tsx,scss,json}\" && npm run lint:precommit && npm run format:precommit",
    "precypress:run:junit": "npm run cypress:delete:reports",
    "prepare": "cd .. && husky install frontend/.husky",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss,json}\"",
    "start": "if [ \"${KIALI_ENV}\" = \"production\" ]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "sh -ac '. ./.env.upstream; npm run start:kiali'",
    "start:kiali": "npm run i18n && REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name REACT_APP_GIT_HASH=$(git rev-parse HEAD) react-scripts start",
    "start:prod": "sh -ac '. ./.env.downstream; npm run start:kiali'",
    "test": "tsc -p . && TEST_RUNNER=1 react-scripts test --env=jsdom __tests__ --transformIgnorePatterns='node_modules/(?!my-library-dir)/'"
  },
  "dependencies": {
    "@patternfly/patternfly": "^5.4.0",
    "@patternfly/react-charts": "^7.4.0",
    "@patternfly/react-core": "^5.4.0",
    "@patternfly/react-table": "^5.4.0",
    "@patternfly/react-topology": "^5.4.1",
    "axios": "^1.11.0",
    "bootstrap-slider-without-jquery": "10.0.0",
    "deep-freeze": "0.0.1",
    "eventemitter3": "4.0.7",
    "i18next": "^23.10.1",
    "i18next-http-backend": "^2.5.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "micro-memoize": "4.0.9",
    "moment": "^2.29.4",
    "react": "^17.0.1",
    "react-ace": "9.1.3",
    "react-copy-to-clipboard": "5.x",
    "react-datepicker": "3.8.0",
    "react-dom": "^17.0.1",
    "react-flexview": "4.0.3",
    "react-i18next": "^11.7.3",
    "react-redux": "7.2.2",
    "react-resize-detector": "9.1.1",
    "react-router": "5.3.x",
    "react-router-dom": "5.3.x",
    "react-router-dom-v5-compat": "^6.11.2",
    "react-virtualized": "9.x",
    "redux": "4.0.3",
    "redux-persist": "5.10.0",
    "redux-persist-transform-filter": "^0.0.22",
    "redux-thunk": "2.4.1",
    "regression": "^2.0.1",
    "reselect": "4.0.0",
    "screenfull": "5.0.2",
    "typesafe-actions": "^4.2.1",
    "typestyle": "^2.4.0",
    "visibilityjs": "2.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.5",
    "@types/deep-freeze": "^0.1.5",
    "@types/enzyme": "3.10.5",
    "@types/jest": "23.3.10",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "4.17.x",
    "@types/node": "^20.14.9",
    "@types/react": "^17.0.1",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.2.0",
    "@types/react-virtualized": "9.x",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.6.1",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-react-selector": "^3.0.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.4.4",
    "husky": "^8.0.3",
    "i18next-parser": "^9.0.1",
    "jest-canvas-mock": "2.2.0",
    "junit-report-merger": "^3.0.5",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react-scripts": "^5.0.1",
    "redux-mock-store": "1.5.4",
    "sass": "^1.63.6",
    "seedrandom": "^3.0.5",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "esbuild": "^0.25.2",
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    ">10%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/integration/**/*.{js,ts}",
    "filterSpecs": true
  }
}
